<body>
  
<div id="snippets-container">
  {{#if isLoggedIn.isLoggedIn}}
    <p>Logged in as {{isLoggedIn.username}}.</p>
    {{/if}}

  <div id="code-snippets-links">
    {{#if viewData.codeSnippets.length}}
    <ul>
      {{#each viewData.codeSnippets}}
        <li>
        <a href="crud/{{this.id}}">
        {{this.title}}
      </a>
      </li>
      {{/each}}
    </ul>
      {{else}}
        <p>No code snippets added yet.</p>
    {{/if}}
</div>

</div>
</body>
</html>